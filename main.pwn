            ShowPlayerMessage(i, "Tu vehiculo de trabajo ha sido destruido. Mision fallida.");
            ResetTrabajo(i);
            break;
        }
    }
        
		if (GLOBAL_VEHICLES[vehicleid][gb_vehicle_TEMP])
	{
