        SetTimerEx("RobberyInProgress", ROB_TIME * 1000, false, "dd", playerid, storeid);
        // INICIA LA ANIMACIÓN
        //ogglePlayerControllable(playerid, 0); // Esto detiene el movimiento/entrada del jugador

