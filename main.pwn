	if(PI[player][pi_VIP])
	{
		inline OnInfoQueryLoad()
		{
			inline OnCountQueryLoad()
			{
				new rows;
				if(cache_get_row_count(rows))
				{
					if(rows)
					{
						cache_get_value_index(0, 0, PI[player][pi_VIP_EXPIRE_DATE]);
