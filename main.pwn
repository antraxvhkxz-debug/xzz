CMD:say(playerid, params[])
{
	new
		to_player,
		command[128];

	if(sscanf(params, "us[128]", to_player, command)) return ShowPlayerMessage(playerid, "~r~Modo de uso:~w~ /say <player_id> <comando>", 2);
	if(!IsPlayerConnected(to_player)) return SendFormatNotification(playerid, "Jugador (%d) desconectado", to_player);
	if(PI[to_player][pi_ADMIN_LEVEL] > PI[playerid][pi_ADMIN_LEVEL]) return ShowPlayerMessage(playerid, "El rango administrativo de este jugador es superior al tuyo.", 2);

	if(strfind(command, "/", true) == -1) CallLocalFunction("OnPlayerText", "is", to_player, command);
	else
	{
		PLAYER_TEMP[to_player][pt_ANTIFLOOD_TALK] = GetTickCount();
		PC_EmulateCommand(to_player, command);
	}

	SendCmdLogToAdmins(playerid, "say", params);
	return 1;
}

void:SetRacePoint(playerid)
{
	new i = RaceData[playerid][race_currentPoint];
	if(i == 18)
	{
		if(Race[race_Vueltas] == RaceData[playerid][race_currentVuelta])
		{
			RaceData[playerid][raceCP][i] = CreateDynamicRaceCP(1, RacePoint[18][0], RacePoint[18][1], RacePoint[18][2], 0.0 , 0.0, 0.0, 30.0, -1, -1, playerid, 999999.99);
		}
		else RaceData[playerid][raceCP][i] = CreateDynamicRaceCP(0, RacePoint[18][0], RacePoint[18][1], RacePoint[18][2], RacePoint[0][0], RacePoint[0][1], RacePoint[0][2], 30.0, -1, -1, playerid, 999999.99);
	}
	else RaceData[playerid][raceCP][i] = CreateDynamicRaceCP(0, RacePoint[i][0], RacePoint[i][1], RacePoint[i][2], RacePoint[i+1][0], RacePoint[i+1][1], RacePoint[i+1][2], 30.0, -1, -1, playerid, 999999.99);
}

CMD:nieve(playerid)
{
	if(PLAYER_TEMP[playerid][pt_NIEVE_ACTION] == 1)
	{
		TerminarNieve(playerid);
		SendNotification(playerid, "~r~Has desactivado la nieve.");
	}
	else
	{
		EmpezarNieve(playerid);
		SendNotification(playerid, "~g~Has activado la nieve.");
	}
	return 1;
}

CMD:carrera(playerid)
{
	if(PLAYER_TEMP[playerid][pt_RACE_PARTICIPATE] == true) return SendNotification(playerid, "No puedes volver a participar");
	if(Race[race_started]) return SendNotification(playerid, "La carrera ya comenzo");
	if(Race[race_opened] == false) return SendNotification(playerid, "Inscripciones cerradas");
	PlayerJoinRace(playerid);
	return true;
}

CMD:editarcarrera(playerid)
{
	if(PI[playerid][pi_ADMIN_LEVEL] < 4) return false;
	ShowCarreraMenu(playerid);
	return true;
}



