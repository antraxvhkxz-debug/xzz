	TextDrawSetProportional(playerid, g_DistanceIconTD[playerid], 0);
   
    // --- 2. TEXTDRAW DEL TEXTO (Distancia) ---
    g_DistanceTD[playerid] = TextDrawCreate(TextX, TextY, "Distancia: ~n~0 metros");
    
    TextDrawAlignment(g_DistanceTD[playerid], 1); 
    TextDrawLetterSize(g_DistanceTD[playerid], 0.20, 1.0); 
    TextDrawColor(g_DistanceTD[playerid], 0xFFFFFFFF);     
    TextDrawSetOutline(g_DistanceTD[playerid], 1);
    TextDrawSetShadow(g_DistanceTD[playerid], 0);
    TextDrawFont(g_DistanceTD[playerid], 2); 
    TextDrawSetProportional(g_DistanceTD[playerid], 1);
    
    // Ocultamos ambos por defecto
    TextDrawHideForPlayer(playerid, g_DistanceIconTD[playerid]);
    TextDrawHideForPlayer(playerid, g_DistanceTD[playerid]);
}

stock DestroyDistanceTextDraw(playerid)
{
    if(g_DistanceIconTD[playerid] != Text:0) {
        TextDrawDestroy(g_DistanceIconTD[playerid]);
        g_DistanceIconTD[playerid] = Text:0;
    }
    if(g_DistanceTD[playerid] != Text:0) {
        TextDrawDestroy(g_DistanceTD[playerid]);
        g_DistanceTD[playerid] = Text:0;
    }
}

// --- 4. LÓGICA DE ACTUALIZACIÓN ---

stock UpdateDistanceTextDraw(playerid)
{
    // Verificar si hay un marcador activo (coordenada no es 0.0)
    if (g_MapTargetX[playerid] != 0.0 || g_MapTargetY[playerid] != 0.0)
    {
        new Float:playerX, Float:playerY, Float:playerZ;
        GetPlayerPos(playerid, playerX, playerY, playerZ);
        
        // Calcular la distancia usando nuestra función 2D personalizada
        new Float:distance = GetDistance2D(playerX, playerY, g_MapTargetX[playerid], g_MapTargetY[playerid]);
        
        new distanceText[MAX_DIST_TD_TEXT];
        
        if (distance < 10.0) // Si está muy cerca (menos de 5 metros)
        {
            // Reiniciar las variables de destino y ocultar el TextDraw
            g_MapTargetX[playerid] = 0.0; // Desactivar el target
            g_MapTargetY[playerid] = 0.0;
            TextDrawHideForPlayer(playerid, g_DistanceIconTD[playerid]);
            TextDrawHideForPlayer(playerid, g_DistanceTD[playerid]);
            return 1;
        }
        else if (distance < 100.0)
        {
            // Menos de 100m, mostrar en metros con decimales
            format(distanceText, sizeof(distanceText), "destino: ~n~~g~%.1f metros", distance);
        }
        else if (distance < 1000.0)
        {
            // Menos de 1km, mostrar en metros (redondeado)
            format(distanceText, sizeof(distanceText), "destino: ~n~~g~%d metros", floatround(distance));
        }
        else
        {
            // Más de 1km, mostrar en kilómetros
            format(distanceText, sizeof(distanceText), "destino: ~n~~g~%.2f km", distance / 1000.0);
        }
        
        // Actualizar el TextDraw con la nueva distancia
        TextDrawSetString(g_DistanceTD[playerid], distanceText);
        
        // Si el TextDraw está oculto, lo mostramos
        //TextDrawShowForPlayer(playerid, g_DistanceIconTD[playerid]);
        TextDrawShowForPlayer(playerid, g_DistanceTD[playerid]);
    }
    return 1;
}


