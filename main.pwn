			for(new i = 0; i != MAX_SU_TOYS; i ++) RemovePlayerAttachedObject(playerid, i);
			SetWantedPlayerMarkers(playerid);
		}
	}

	PLAYER_TEMP[playerid][pt_WORKING_FACT] = fact;
	return 1;
}

