// --- FUNCIÓN AUXILIAR PARA CALCULAR Y DAR FORMATO AL TIEMPO ---
stock GetRemainingTimeFormatted(seconds, output[], size)
{
    if (seconds <= 0)
    {
        format(output, size, "¡LISTO PARA RECLAMAR AHORA!");
        return 1;
    }
    
    new hours = seconds / 3600;
    new minutes = (seconds % 3600) / 60;
    new remaining_seconds = seconds % 60;

    format(output, size, "%02d horas, %02d minutos y %02d segundos", hours, minutes, remaining_seconds);
    return 1;
}



