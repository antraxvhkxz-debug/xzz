			{
				if (pvehicles >= MAX_NU_VEHICLES)
				{
					ShowPlayerMessage(playerid, "No puedes tener mas vehiculos, tu lemite es %d, con ~y~/vip~w~ puedes tener %d ~y~vehiculos~w~.", MAX_NU_VEHICLES, MAX_SU_VEHICLES);
