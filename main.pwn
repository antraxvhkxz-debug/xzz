       { 
       SetPlayerArmourEx(playerid, 0.0);
       SetPlayerHealthEx(playerid, 20.0); 
        PI[playerid][pi_CHEAT] = 0;	
        }
        
        if(PI[playerid][pi_KANA] == 1) 
       { 
            ShowPlayerMessage(playerid, "Saliste del servidor con esposas, has sido encarcelado", 3);
