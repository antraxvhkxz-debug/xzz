	new string[210];
	InfoMochila[id][mModelo] = 0;
	Loop(x, 9, 0)
	{
		InfoMochila[id][mSlot][x] = 0;
		InfoMochila[id][mSlot_Cant][x] = 0;
		InfoMochila[id][mSlot_Tipo][x] = 0;
	}
	mysql_format(srp_db, string, sizeof(string), "DELETE FROM `mochilas` WHERE `ID` = '%d'", InfoMochila[id][mID]);
