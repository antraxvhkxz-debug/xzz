forward ComenzarCarrera();
public ComenzarCarrera()
{
	for(new i = 0, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(RaceData[i][race_Inrace] == true)
		{
			if(IsPlayerInAnyVehicle(i))
			{
				TogglePlayerControllable(i, true);
				SetRacePoint(i);
				GameTextForPlayer(i, "Fuera!", 1000, 1);
			}
		}
	}
	return true;
}

forward FinalizarCarrera();
public FinalizarCarrera()
{
	for(new i = 0, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(RaceData[i][race_Inrace] == true)
		{
			//SetPlayerPosEx(i, 1796.9125,842.8305,10.6328);
			SendNotification(i, "La carrera finalizo");
			RaceData[i][race_Inrace] = false;

			switch(RaceData[i][race_Lugar])
			{
				case 1:
				{
					SetPlayerPos(i, 1806.7825,865.5578,13.7099);
					SetPlayerFacingAngle(i, 180);
					RaceData[i][race_Lugar] = 0;

				}
				case 2:
				{
					SetPlayerPos(i, 1809.5748,865.4629,12.8399);
					SetPlayerFacingAngle(i, 180);
					RaceData[i][race_Lugar] = 0;

				}
				case 3:
				{
					SetPlayerPos(i, 1803.3219,865.8015,12.1699);
					SetPlayerFacingAngle(i, 180);
					RaceData[i][race_Lugar] = 0;

				}
				default: {
					SetPlayerPos(i, 1796.9125,842.8305,10.6328);
					RaceData[i][race_Lugar] = 0;

				}
			}
		}
		for(new z = 0; z < 19; z++)
		{
			if(IsValidDynamicRaceCP(RaceData[i][raceCP][z]))
			{
				DestroyDynamicRaceCP(RaceData[i][raceCP][z]);
			}
			RaceData[i][raceCP][z]  = INVALID_STREAMER_ID;
		}
		RaceData[i][race_currentPoint] = 0;
	}

	for(new d = 0; d < 12; d++)
	{
		Race[race_places][d] = false;
		if(IsValidVehicle(Race[race_Vehicles][d]))
		{
			DestroyVehicleEx(Race[race_Vehicles][d]);
		}
	}
	Race[raceFinished] = 0;
	return true;
}

new countdown = 5;

forward ConteoRegresivo();
public ConteoRegresivo()
{
	if(countdown == 0)
	{
		KillTimer(Race[racetimer]);
		ComenzarCarrera();
		countdown = 5;
		return true;
	}
	new string[10];
	format(string, sizeof(string), "Conteo %d", countdown);

	for(new d = 0, j = GetPlayerPoolSize(); d <= j; d++)
	{
		if(RaceData[d][race_Inrace] == true)
		{
			GameTextForPlayer(d, string, 1000, 4);
		}
	}
	countdown--;
	return true;
}

CMD:regalo(playerid, params[])
{

	new item[20], target;
	if(PI[playerid][pi_ADMIN_LEVEL] < 4) return false;
	if(sscanf(params, "s[128]I(-1)", item, target)) return SendNotification(playerid, "~r~Modo de uso:~w~ /regalo [crear - quitar - ir]");
	if(strcmp(item, "crear", true)  == 0)
	{
		for(new i = 0; i < 5; i ++)
		{
			if(Regalos[i][regaloX] == 0.0)
			{
				GetPlayerPos(playerid, Regalos[i][regaloX], Regalos[i][regaloY], Regalos[i][regaloZ]);
				GetPlayerFacingAngle(playerid, Regalos[i][regaloRot]);
				
				Regalos[i][regaloObject] = CreateDynamicPickup(1276, 21 ,  Regalos[i][regaloX], Regalos[i][regaloY], Regalos[i][regaloZ] -0.4, -1, -1);
				//Regalos[i][regalo3DText] = Create3DTextLabel("{00ee33}{FFFFFF}Regalo Disponible!\n {00ee33}{FFFFFF}/recibir regalo{FFFFFF} Para recibir el regalo", -1, Regalos[i][regaloX], Regalos[i][regaloY], Regalos[i][regaloZ], 8.0);
				SetPlayerPos(playerid, Regalos[i][regaloX], Regalos[i][regaloY], Regalos[i][regaloZ]+2);
				return true;
			}
		}
		return SendNotification(playerid, "~r~Atención~w~ No hay más slots de regalos disponibles");
	}
	else if(strcmp(item, "ir", true) == 0)
	{
		if(target == -1) return SendNotification(playerid, "~r~Atención~w~ Para esta opción ingresa la ID de alguna caja /regalo ir <id>");
		if(target < 0 || target > 5) return SendNotification(playerid, "~r~Atención~w~ Las ID's son enter < 0 A 4>");
		if(Regalos[target][regaloX] != 0.0)
		{
			SetPlayerPos(playerid, Regalos[target][regaloX], Regalos[target][regaloY], Regalos[target][regaloZ]+2);
		}
		else return SendNotification(playerid, "~r~Atención~w~ ID Incorrecta");
		return true;
	}
	else if(strcmp(item, "quitar", true) == 0)
	{
		for(new i = 0; i < 5; i++)
		{
			if(IsPlayerInRangeOfPoint(playerid, 2.5, Regalos[i][regaloX], Regalos[i][regaloY], Regalos[i][regaloZ]))
			{

				Regalos[i][regaloX] = 0.0;
				Regalos[i][regaloY] = 0.0;
				Regalos[i][regaloZ] = 0.0;
				Regalos[i][regaloRot] = 0.0;
			
				DestroyDynamicPickup(Regalos[i][regaloObject]);
				if(IsValidDynamic3DTextLabel(Regalos[i][regalo3DText])) DestroyDynamic3DTextLabel(Regalos[i][regalo3DText]);

				return true;
			}
		}
		return SendNotification(playerid, "~r~Atención~w~ No estas en un rango de 2.5m del regalo");

	}
	else {
		return SendNotification(playerid, "~r~Atención~w~ Opción desconocida");
	}

}

