forward VehiclesTime();
public VehiclesTime()
{
	for(new i; i <= GetVehiclePoolSize(); i++)
	{
		if(IsValidVehicle(i))
		{
			if(GLOBAL_VEHICLES[i][gb_vehicle_IS_RETURN])
			{

				new link = GLOBAL_VEHICLES[i][gb_vehicle_LINK_ID];

				if(link >= 0 && link < sizeof(San_Andreas_Vehicles))
				{
					if(GetVehicleDistanceFromPoint(i, San_Andreas_Vehicles[link][san_andreas_vehicle_SPAWN_X], San_Andreas_Vehicles[link][san_andreas_vehicle_SPAWN_Y], San_Andreas_Vehicles[link][san_andreas_vehicle_SPAWN_Z]) <= 6.0)
					{
						GLOBAL_VEHICLES[i][gb_vehicle_TIME_OUT] ++;

						if(GLOBAL_VEHICLES[i][gb_vehicle_TIME_OUT] >= 40)
						{
							SetVehicleToRespawnEx(i);
						}
					}
					else
					{
						if(GLOBAL_VEHICLES[i][gb_vehicle_TIME_OUT] > 0)
						{
							GLOBAL_VEHICLES[i][gb_vehicle_TIME_OUT] = 0;
						}
					}
				}
			}

				else
				{
					if(GLOBAL_VEHICLES[i][gb_vehicle_TIME_OUT] > 0)
					{
						GLOBAL_VEHICLES[i][gb_vehicle_TIME_OUT] = 0;
					}
				}
			}
		}
	return 1;
}

