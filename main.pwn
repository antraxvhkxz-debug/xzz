forward PlayerJoinRace(playerid);
public PlayerJoinRace(playerid)
{
	for(new i = 0; i < 12; i++)
	{
		if(Race[race_places][i] == false)
		{
			if(Race[race_Vueltas] == 0) return SendNotification(playerid, "La carrera está siendo configurada");
			if(Race[race_Vehicle] == 0) return SendNotification(playerid, "La carrera está siendo configurada");
			RaceData[playerid][race_currentVuelta] = 1;
			RaceData[playerid][race_currentPoint] = 0;
			new vid = AddTestVehicle(false, Race[race_Vehicle], RacePlaces[i][0], RacePlaces[i][1], RacePlaces[i][2], RacePlaces[i][3], 1, 1, 0 , 0);
			PutPlayerInVehicleEx(playerid, vid, 0);
			GLOBAL_VEHICLES[vid][gb_vehicle_PARAMS_ENGINE] = 1;
			UpdateVehicleParams(vid);
			Race[race_Vehicles][i] = vid;
			SendNotification(playerid, "La carrera iniciará pronto");
			TogglePlayerControllable(playerid, false);
			RaceData[playerid][race_Inrace] = true;
			Race[race_places][i] = true;
			RaceData[playerid][race_Place] = i;
			PLAYER_TEMP[playerid][pt_RACE_PARTICIPATE] = true;
			return true;
		}
	}
	SendNotification(playerid,"No hay espacios disponibles");
	return false;
}

