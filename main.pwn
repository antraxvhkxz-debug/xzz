stock CheckDailyReward(playerid)
{
    new currentTime = gettime();
    new lastTime = g_LastRewardTime[playerid];
    new remainingSeconds = (lastTime + REWARD_COOLDOWN) - currentTime;
    
    // Declaraciones de diálogo
    new header[128];
    new rewardInfo[MAX_REWARD_TEXT];
    new rewardList[MAX_REWARD_TEXT]; 
    
    // Inicialización de buffers (crucial para evitar crash en nuevo usuario)
    rewardInfo[0] = EOS; 
    rewardList[0] = EOS; 
    
    // --- 1. SI EL COOLDOWN ESTÁ ACTIVO (remainingSeconds > 0) ---
    if (remainingSeconds > 0)
    {
        new remainingTimeFormatted[64];
        
        GetRemainingTimeFormatted(remainingSeconds, remainingTimeFormatted, sizeof(remainingTimeFormatted));
        
        format(header, sizeof(header), "{FFFD46}[Recompensas] {ffffff}  Día %d{FFFFFF}", g_RewardDay[playerid]);
        
        // Bloque de formato COOLDOWN (Corregido y en una línea)
        format(rewardInfo, sizeof(rewardInfo), 
            ""WHITE"Actualmente vas en el {00FF00}Día %d{FFFFFF} del ciclo.\n\n{FFFF00}TIEMPO RESTANTE:{FFFFFF}\n{FF0000}%s{FFFFFF}",
            g_RewardDay[playerid], 
            remainingTimeFormatted 
        );

        ShowPlayerDialog(playerid, DIALOG_DAILY_REWARD_COOLDOWN, DIALOG_STYLE_MSGBOX, header, rewardInfo, "Ok", "");
        return 1;
    }
    
    // --- 2. SI LA RECOMPENSA ESTÁ LISTA (remainingSeconds <= 0) ---

    new i;
    new day_status[64];
    new day_entry[128];
    
    // El loop va desde 0 hasta MAX_REWARD_DAYS - 1
    for (i = 0; i < MAX_REWARD_DAYS; i++)
    {
        // 1. Determinar el estado del día (i + 1)
        if (i + 1 < g_RewardDay[playerid])
        {
            // Día ya reclamado (anterior al día actual)
            format(day_status, sizeof(day_status), "{00FF00}[RECLAMADO]{FFFFFF}");
        }
        else if (i + 1 == g_RewardDay[playerid])
        {
            // Día actual, listo para reclamar
            format(day_status, sizeof(day_status), "{FFFF00}[HOY]{FFFFFF}");
        }
        else
        {
            // Día futuro (pendiente)
            format(day_status, sizeof(day_status), "{FF0000}[PENDIENTE]{FFFFFF}");
        }

        // 2. Formatear la entrada completa para este día
        // NOTA: Usamos \t\t para intentar alinear la columna de estado
        format(day_entry, sizeof(day_entry), 
            "Día %d: ${BFFFFF}%d\t\t%s\n", 
            i + 1, // Número de Día
            RewardAmounts[i], // Monto de Recompensa (índice 0 = Día 1)
            day_status
        );

        // 3. Añadir la entrada a la cadena principal (rewardList)
        strcat(rewardList, day_entry);
    }
    
    format(header, sizeof(header), "{FFFD46}[Recompensas] {ffffff}  Día %d{FFFFFF}", g_RewardDay[playerid]);
    
    // Bloque de formato REWARD READY (Corregido y en una línea)
    format(rewardInfo, sizeof(rewardInfo), 
        ""WHITE"¡Bienvenido! Reclama la recompensa del {00FF00}Día %d{FFFFFF} y avanza en el ciclo.\n\n"WHITE"--- PROGRESO DEL CICLO ---\n%s\n"WHITE"---------------------------\n"WHITE"Recompensa a reclamar ahora: {00FF00}${FFFFFF}%d",
        g_RewardDay[playerid],
        rewardList,
        RewardAmounts[g_RewardDay[playerid] - 1]
    );

    // Usamos DIALOG_STYLE_MSGBOX
    ShowPlayerDialog(playerid, DIALOG_DAILY_REWARD, DIALOG_STYLE_MSGBOX, header, rewardInfo, "Reclamar", "xCerrar");

    return 1;
}

// --- FUNCIÓN PARA PROCESAR LA RECLAMACIÓN DE RECOMPENSA ---
stock ClaimPlayerDailyReward(playerid)
{
    // 1. Obtener la cantidad de recompensa del día actual
    new currentRewardIndex = g_RewardDay[playerid] - 1;
    new rewardAmount = RewardAmounts[currentRewardIndex];
    
    // ** Importante: Reemplaza esto con tu función real de dar dinero **
    GivePlayerCash(playerid, rewardAmount);
    
    // 2. Actualizar el tiempo y el día
    g_LastRewardTime[playerid] = gettime(); // Reiniciar el cooldown
    g_RewardDay[playerid]++; // Avanzar al siguiente día
    
    // 3. Reiniciar el ciclo si se ha completado
    if (g_RewardDay[playerid] > MAX_REWARD_DAYS)
    {
        g_RewardDay[playerid] = 1; // Volver al Día 1
    }
    
    // 4. Notificar y Guardar (Inmediatamente)
    SendFormatNotification(playerid, "~w~Recompensa ~g~reclamada.");
    SavePlayerRewardData(playerid); // Guardar el nuevo tiempo y día.
}

