		SetVehicleSpawnInfo(vehicleid, GetVehicleModel(vehicleid), pos_v[0], pos_v[1], pos_v[2], pos_v[3], colors[0], colors[1]);
	}
	return SetVehicleToRespawn(vehicleid);
}


