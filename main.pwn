		} else {
			if (PI[playerid][pi_VIP]) format(str, sizeof(str), "{66c4e4}[Twitter] - [{D3D3D3}%d{66c4e4}][{D3D3D3}@{323232}ANONIMO{66c4e4}][{FFE000}VIP{66c4e4}]{D3D3D3}: %s", playerid, message);
