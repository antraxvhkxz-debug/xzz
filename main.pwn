public OnPlayerSpawn(playerid)
{
    EmpezarNieve(playerid);
    
        // --- LIMPIEZA INICIAL ---
    // ¡Usamos DestroyDynamic3DTextLabel porque vamos a ser gente decente!
    if(LabelNombre[playerid] != Text3D:INVALID_3DTEXT_ID) {
        DestroyDynamic3DTextLabel(LabelNombre[playerid]);
        LabelNombre[playerid] = Text3D:INVALID_3DTEXT_ID;
    }

    new string[512], name[MAX_PLAYER_NAME], vip_label[30], extra_info[100];
    GetPlayerName(playerid, name, sizeof(name));
    
    // Limpiamos el nombre (quitar guiones bajos)
    for(new i = 0; i < strlen(name); i++) {
        if(name[i] == '_') name[i] = ' ';
    }

    // 1. LÓGICA DE TRABAJO O BANDA
    extra_info[0] = '\0'; 
    if(PLAYER_WORKS[playerid][WORK_POLICE][pwork_SET]) 
    {
        format(extra_info, sizeof(extra_info), "{0063FF}Policía\n");
    }
    else 
    {
        new crew_idx = GetCrewIndexById(PI[playerid][pi_CREW]);
        if(crew_idx != -1) 
        {
            format(extra_info, sizeof(extra_info), "{%06x}%s\n", 
                CREW_INFO[crew_idx][crew_COLOR] >>> 8, 
                CREW_INFO[crew_idx][crew_NAME]);
        }
    }

    // 2. LÓGICA DE VIP
    if(PI[playerid][pi_VIP] > 0) format(vip_label, sizeof(vip_label), "{FFFF00}VIP ");
    else vip_label[0] = '\0';
    
    // 3. OBTENER VIDA Y CHALECO
    new Float:hp, Float:armour;
    GetPlayerHealth(playerid, hp);
    GetPlayerArmour(playerid, armour);

    // 4. FORMATEO FINAL UNIFICADO
    format(string, sizeof(string), "%s%s %s"WHITE"%s (%d)\n{FF4B4B}%.0f HP %s{FFFFFF}%.0f AP", 
        extra_info,
        (PLAYER_TEMP[playerid][pt_MODE] ? "{00ACFF}" : "{E936FC}"), 
        vip_label, 
        name, 
        playerid,
        hp,
        (armour > 0.5) ? "{C0C0C0}" : "{555555}",
        armour
    );

    // 5. CREAR EL LABEL (¡DINÁMICO, CABRÓN!)
    // Parámetros: Texto, Color, X, Y, Z, Distancia, JugadorPegado, VehiculoPegado, TestLineOfSight
    // Al poner 'playerid' como penúltimo parámetro, SE PEGA SOLO. No necesitas Attach.
    LabelNombre[playerid] = CreateDynamic3DTextLabel(string, 0xFFFFFFFF, 0.0, 0.0, 0.35, 12.0, playerid, INVALID_VEHICLE_ID, 1);

    
    PLAYER_TEMP[playerid][pt_WORK_VEHICLE_SPAWN_TIMER] = gettime() + 15;
	
	if(!PLAYER_TEMP[playerid][pt_USER_LOGGED])
	{
		printf("[WARNING] playerid: %i, SPAWNED but it's not correct.", playerid);
		return Kick(playerid);
	}

	for(new i = 0; i < MAX_ANUNCIOS; i++)
		{
			TextDrawSetString(_InfoAnuncio[i][TDAnuncio], _InfoAnuncio[i][Anuncio]);
			TextDrawShowForPlayer(playerid, _InfoAnuncio[i][TDAnuncio]);
		}

	if(PLAYER_TEMP[playerid][pt_GAME_STATE] == GAME_STATE_CONNECTED) // Primer spawn
	{
		if(PLAYER_TEMP[playerid][pt_LOGIN_KICK_TIMER] != -1) {
			KillTimer(PLAYER_TEMP[playerid][pt_LOGIN_KICK_TIMER]);
			PLAYER_TEMP[playerid][pt_LOGIN_KICK_TIMER] = -1;
		}

		if(IsPlayerUsingAndroid(playerid))
		{
	  		PLAYER_TEMP[playerid][pt_MODE] = MODE_ANDROID;
		}
		else
		{
